package aplicacion.modelo.events;
import java.awt.event.ItemEvent;
import javax.swing.event.ChangeListener;
import javax.swing.event.ChangeEvent;
public class _ChangeListener implements ChangeListener{

public _ChangeListenerHandler _handler=null;
	
public void stateChanged(ChangeEvent e){
	this._handler.procesar(e);
}
	public void setEventHandler(_ChangeListenerHandler handler){
		this._handler=handler;
	}
	
}
